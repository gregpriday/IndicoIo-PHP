{
    "name": "gregpriday/indicoio-php",
    "description": "Modified version of the Indico PHP Rest API wrapper.",
    "version": "0.6.2",
    "keywords": ["Rest Api Wrapper for PHP", "IndicoIo"],
    "type": "project",
    "require": {
        "php": ">=7.0",
        "eventviva/php-image-resize": "1.5.*",
        "ext-curl": "*"
    },
	"require-dev": {
        "phpunit/phpunit": "7.*"
    },
    "autoload": {
        "psr-4": {"IndicoIo\\": "IndicoIo/"}
    },
    "autoload-dev": {
        "psr-4": {"IndicoIo\\Test\\": "tests/"}
    },
    "minimum-stability": "dev"
}
